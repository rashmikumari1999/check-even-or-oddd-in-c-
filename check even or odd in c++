#include <iostream>
using namespace std;
int main()
{
    int num;
    cout<<"check the number is even or odd";
cout<<"...................../n/n";
cout<<"enter  a number";
cin>>num;
if(num%2==0)
{
    cout<<"number is even";
}
else
{
    cout<<"number is odd";
}
    return 0;
}
